<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <custom-type type-id="NewsletterSubscription">
        <display-name xml:lang="x-default">Newsletter</display-name>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>site</storage-scope>
        <key-definition attribute-id="email">
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="firstName">
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="lastName">
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="phone">
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="newsletter">
                <display-name xml:lang="x-default">Newsletter Group</display-name>
                <attribute attribute-id="email"/>
                <attribute attribute-id="lastName"/>
                <attribute attribute-id="firstName"/>
                <attribute attribute-id="phone"/>
            </attribute-group>
        </group-definitions>
    </custom-type>

    <custom-type type-id="mycustomobject">
        <display-name xml:lang="x-default">The Custom Object</display-name>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>organization</storage-scope>
        <key-definition attribute-id="customkey">
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <group-definitions>
            <attribute-group group-id="mygroupid">
                <display-name xml:lang="x-default">My group</display-name>
                <attribute attribute-id="creationDate" system="true"/>
                <attribute attribute-id="customkey"/>
            </attribute-group>
        </group-definitions>
    </custom-type>

</metadata>
